{
    "name": "yedincisenol/bkm-express",
    "type": "library",
    "description": "Bex PHP Client Library",
    "authors": [
        {
            "name": "Bex Developer Team"
        },
        {
            "name": "İbrahim Ş. Örencik"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-0": {
            "Bex": "src/main/"
        },
        "psr-4": {
            "Bex\\": "src/main/OldBex"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "BexTest": "src/test/"
        },
        "psr-4": {
            "BexTest\\": "src/test/BexTest"
        }
    }
}
